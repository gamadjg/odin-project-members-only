<div class="formMain">
	<form action="/account/log-in" method="POST">
		<label for="username" class="formLabel">Email</label>
		<input
			name="username"
			type="email"
			placeholder="Email"
			id="username"
			required
		/>
		<label for="password" class="formLabel">Password</label>
		<input
			name="password"
			type="password"
			placeholder="Password"
			id="password"
			required
		/>
		<button>Log In</button>
	</form>
</div>
